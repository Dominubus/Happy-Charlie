<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="ExtraText">
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context=".musica"
        tools:showIn="@layout/activity_musica">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="2dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:ignore="UselessParent">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/avatarmusica">

                    <ImageButton
                        android:id="@+id/salir_musica"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:onClick="irLobby"
                        app:srcCompat="@drawable/atras"
                        tools:ignore="ContentDescription" />


                </FrameLayout>

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        tools:ignore="ScrollViewSize">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion1">

                                <Button
                                    android:id="@+id/cancion1"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Don't Stop Me Now"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />


                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion2">

                                <Button
                                    android:id="@+id/cancion2"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Dancing Queen"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion3">

                                <Button
                                    android:id="@+id/cancion3"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Good Vibrations"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion4">

                                <Button
                                    android:id="@+id/cancion4"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Uptown Girl"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion5">

                                <Button
                                    android:id="@+id/cancion5"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Eye Of The Tiger"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion6">

                                <Button
                                    android:id="@+id/cancion6"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="I'm a Believer"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    tools:ignore="HardcodedText" />
                                android:textColorHint="#2B0606"/>_

                            </FrameLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion7">

                                <Button
                                    android:id="@+id/cancion7"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Girls Just Want To Have Fun"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion8">

                                <Button
                                    android:id="@+id/cancion8"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Living On A Prayer"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion9">

                                <Button
                                    android:id="@+id/cancion9"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="I Will Survive"
                                    android:textColor="#144770"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <FrameLayout
                                android:layout_width="132dp"
                                android:layout_height="132dp"
                                android:background="@drawable/cancion10">

                                <Button
                                    android:id="@+id/cancion10"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="#5CFFFFFF"
                                    android:onClick="IrPantallaReproductor"
                                    android:text="Walking On Sunshine"
                                    android:textColor="#144770"
                                    android:textColorHighlight="#FFFFFF"
                                    android:textColorHint="#2B0606"
                                    tools:ignore="HardcodedText" />

                            </FrameLayout>

                        </LinearLayout>

                    </LinearLayout>
                </ScrollView>

            </LinearLayout>
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>